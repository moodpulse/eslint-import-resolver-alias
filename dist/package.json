{
  "name": "eslint-import-resolver-locals-alias",
  "version": "2.0.0",
  "description": "A simple Node behavior import resolution plugin for eslint-plugin-import, supporting module alias with local project modules and node_modules.",
  "main": "index.js",
  "scripts": {
    "release": "node build/release.js",
    "test": "mocha test/test.js --require test/setup.js",
    "cov": "istanbul cover node_modules/mocha/bin/_mocha -- test/test.js --require test/setup.js"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "modules",
    "alias",
    "eslint-plugin-import",
    "import-resolver"
  ],
  "engines": {
    "node": ">= 6"
  },
  "author": "Mikhail Privalov",
  "license": "MIT",
  "devDependencies": {
    "adjust-md-for-publish": "^1.0.0",
    "builtin-modules": "^2.0.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^3.2.0"
  },
  "peerDependencies": {
    "eslint-plugin-import": ">=1.4.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moodpulse/eslint-import-resolver-locals-alias.git"
  },
  "bugs": {
    "url": "https://github.com/moodpulse/eslint-import-resolver-locals-alias/issues"
  },
  "homepage": "https://github.com/moodpulse/eslint-import-resolver-locals-alias#readme"
}
